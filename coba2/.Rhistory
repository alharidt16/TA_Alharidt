library(ggplot2)
# Assume you have a data frame called "df" with the following columns:
# - x: the x-axis values
# - y: the y-axis values
# - cluster: the cluster assignment (1, 2, ..., k)
library(readxl)
df<- read_excel("~/GitHub/TA_Alharidt/DATA_TA_HASIL_MEDOIDS.xlsx")
# Create a color palette for the clusters
palette <- rainbow(nlevels(df$cluster_kmodes))
# Create the plot
ggplot(df, aes(x = x, y = y, color = as.factor(cluster))) +
geom_point() +
scale_color_discrete(name = "Cluster", labels = levels(df$cluster)) +
labs(x = "X-axis", y = "Y-axis") +
theme_classic() +
theme(legend.position = "bottom")
